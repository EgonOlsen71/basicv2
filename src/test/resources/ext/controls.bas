10 GRON 640,480:BUFFERMODE 1
11 A$="wasdq"
15 A=LOADSHAPE("src/test/resources/bin/sprite.png")
20 X=320:Y=240
30 CLEAR
40 DRAWSHAPE A,X,Y:FLIP:LIMIT 60
50 GGET A$:IF A$=""GOTO30
60 IF A$="d" THEN X=X+2
70 IF A$="a" THEN X=X-2
80 IF A$="w" THEN Y=Y-2
90 IF A$="s" THEN Y=Y+2
95 IF A$="q" THEN 500
120 GOTO 30
500 GROFF